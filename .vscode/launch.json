    "version": "0.2.0",
    "configurations": [
        {
            "name": "Java",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}",
                "src/main/java/comp3111_project/Library.java"
            ],
            "vmArgs": "--module-path /home/siddarth/COMP 3111/javafx-sdk-19/lib  --add-modules javafx.controls,javafx.fxml",
        },
        {
            "name": "Java Console App",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ],
            "externalConsole": true,
            "vmArgs": "--module-path /home/siddarth/COMP 3111/javafx-sdk-19/lib  --add-modules javafx.controls,javafx.fxml",
      
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}",
            "vmArgs": "--module-path /home/siddarth/COMP 3111/javafx-sdk-19/lib  --add-modules javafx.controls,javafx.fxml",
        },
        {
            "type": "java",
            "name": "Launch Library",
            "request": "launch",
            "mainClass": "comp3111_project.Library",
            "projectName": "comp3111_project",
            "vmArgs": "--module-path /home/siddarth/COMP 3111/javafx-sdk-19/lib  --add-modules javafx.controls,javafx.fxml"
        }
    ]
}